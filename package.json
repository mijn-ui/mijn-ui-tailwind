{
  "name": "mijn-ui-tailwind",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "echo 'Running fumadocs-mdx...' && fumadocs-mdx || echo 'fumadocs-mdx failed'",
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --cache --check --ignore-path .gitignore --ignore-path .prettierignore .",
    "prettier:fix": "prettier --cache --write --list-different --ignore-path .gitignore --ignore-path .prettierignore .",
    "types:check": "tsc --noEmit",
    "clean": "rimraf .next"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "clsx": "^2.1.1",
    "fumadocs-core": "14.5.4",
    "fumadocs-docgen": "^1.3.2",
    "fumadocs-mdx": "11.1.2",
    "fumadocs-ui": "14.5.4",
    "html-react-parser": "^5.1.18",
    "next": "15.0.3",
    "next-themes": "^0.4.4",
    "react": "19.0.0-rc.1",
    "react-click-away-listener": "^2.3.0",
    "react-dom": "19.0.0-rc.1",
    "react-icons": "^5.4.0",
    "react-resizable-panels": "^2.1.7",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/mdx": "^2.0.13",
    "@types/node": "22.9.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "postcss": "^8.4.49",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2"
  }
}
